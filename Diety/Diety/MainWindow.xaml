<Window x:Class="Diety.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" AllowsTransparency="True" ResizeMode="CanResizeWithGrip"
        xmlns:extensions="clr-namespace:Diety.Helpers.Extensions" Left="0" Top="0"
        Title="MainWindow" MinHeight="500" MinWidth="1000" WindowStyle="None"
		 >	
	<Grid>
		<Frame NavigationUIVisibility="Hidden" x:Name="MainFrame" DataContext="{Binding Source={StaticResource Locator}, Path=Main}" />
	</Grid>
</Window>
