<Page x:Class="Diety.Views.Second"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:viewModel="clr-namespace:Diety.ViewModel"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
	Title="Second">
	<Grid DataContext="{Binding Source={StaticResource Locator}, Path=Second}">
		<Button Command="{Binding GoToStart}">
			<TextBlock Text="{Binding SecondText}">				
			</TextBlock>
		</Button>
    </Grid>
</Page>
